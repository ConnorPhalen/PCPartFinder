<Window x:Class="PCfinder2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PCfinder2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="New Tab" HorizontalAlignment="Left" Margin="64,22,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="New Tab 2" HorizontalAlignment="Left" Margin="338,22,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="227" Margin="10,85,0,0" VerticalAlignment="Top" Width="500"/>
        <TextBox x:Name="textBoxSearch" HorizontalAlignment="Left" Height="22" Margin="170,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="141"/>
        <Button x:Name="buttonSearch" Content="Search" HorizontalAlignment="Left" Margin="205,58,0,0" VerticalAlignment="Top" Width="75" Click="buttonSearch_Click"/>

    </Grid>
</Window>
